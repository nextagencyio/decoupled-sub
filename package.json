{
  "name": "decoupled-sub",
  "version": "0.1.0",
  "description": "Subscription-based content platform starter with Stripe integration",
  "private": true,
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "tsx scripts/setup.ts",
    "setup-content": "npx decoupled-cli@latest content import --file data/subscription-content.json",
    "setup-content:preview": "npx decoupled-cli@latest content import --file data/subscription-content.json --preview",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@stripe/stripe-js": "^8.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.539.0",
    "next": "^16.1.6",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stripe": "^20.3.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.20",
    "decoupled-cli": "^2.2.1",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
